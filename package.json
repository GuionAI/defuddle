{
	"name": "@flicknote/defuddle",
	"version": "0.6.6",
	"description": "Extract article content and metadata from web pages (FlickNote fork, optimized for linkedom/Cloudflare Workers)",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"full": [
				"dist/index.full.d.ts"
			]
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./full": {
			"types": "./dist/index.full.d.ts",
			"import": "./dist/index.full.js",
			"default": "./dist/index.full.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build:types": "tsc --project tsconfig.declarations.json",
		"build:js": "node esbuild.config.mjs",
		"build": "npm run clean && npm run build:types && npm run build:js",
		"prepublishOnly": "npm run build",
		"dev:types": "tsc --project tsconfig.declarations.json --watch",
		"dev:js": "node esbuild.config.mjs --dev",
		"dev": "npm run clean && npm run build:types && concurrently \"npm run dev:types\" \"npm run dev:js\"",
		"test": "TZ=UTC vitest run",
		"playground": "node playground/server.js",
		"release": "npx semantic-release"
	},
	"keywords": [
		"readability",
		"content-extraction",
		"article-extraction",
		"web-scraping",
		"html-cleanup",
		"content-parser",
		"article-parser",
		"dom",
		"linkedom",
		"cloudflare-workers",
		"flicknote"
	],
	"author": "FlickNote Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:GuionAI/defuddle.git"
	},
	"optionalDependencies": {
		"mathml-to-latex": "^1.4.3",
		"temml": "^0.11.2",
		"turndown": "^7.2.0"
	},
	"devDependencies": {
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/npm": "^13.1.2",
		"@types/node": "^20.19.0",
		"@types/turndown": "^5.0.5",
		"concurrently": "^8.2.2",
		"esbuild": "^0.27.1",
		"linkedom": "^0.18.9",
		"semantic-release": "^25.0.2",
		"typescript": "^5.3.3",
		"vitest": "^3.2.4"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
